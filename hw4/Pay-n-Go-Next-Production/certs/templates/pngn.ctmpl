{{- with pkiCert "pki/issue/pngn" "common_name=pngn" "ttl=1h" -}}
{{ .Cert }}{{ .CA }}{{ .Key }}
{{ .Key | writeToFile "/secrets/pngn.key" "1001" "65534" "0400" }}
